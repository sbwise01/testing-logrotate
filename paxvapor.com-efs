/home/webadmin/sites/paxvapor.com/common/var/log/*log {
    create 0644 webadmin webadmin
    daily
    rotate 5
    missingok
    notifempty
    compress
    sharedscripts
    postrotate
        /etc/init.d/nginx reopen_logs
        /usr/local/bin/archivetos3.sh pax-production-fogops-logrotation /home/webadmin/sites/paxvapor.com/common/var/log/*log >> /var/log/archivetos3.log 2>&1
    endscript
}
